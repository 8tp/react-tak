{
    "name": "@tak-ps/node-tak",
    "type": "module",
    "version": "11.8.0",
    "description": "Lightweight JavaScript library for communicating with TAK Server",
    "author": "Nick Ingalls <nick@ingalls.ca>",
    "main": "dist/index.node.js",
    "types": "dist/index.d.ts",
    "bin": {
        "tak": "./cli.ts"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "react-native": "./dist/index.native.js",
            "import": "./dist/index.node.js",
            "default": "./dist/index.node.js"
        },
        "./lib/*": "./dist/lib/*.js",
        "./lib/api/*": "./dist/lib/api/*.js"
    },
    "engines": {
        "node": ">= 22"
    },
    "scripts": {
        "test": "vitest run",
        "lint": "eslint *.ts test/ lib/",
        "doc": "typedoc index.ts",
        "build": "tsc --build",
        "pretest": "npm run lint"
    },
    "dependencies": {
        "@openaddresses/batch-error": "^2.12.0",
        "@sinclair/typebox": "^0.34.41",
        "@tak-ps/node-tak": "^11.8.0",
        "ajv": "^8.12.0",
        "eventemitter3": "^5.0.1",
        "form-data": "^4.0.2",
        "mime": "^4.0.7",
        "p12-pem": "^1.0.5",
        "pem": "^1.14.8",
        "undici": "^7.8.0",
        "xml-js": "^1.6.11"
    },
    "peerDependencies": {
        "@tak-ps/node-cot": "^14.7.1",
        "react-native-ssl-pinning": ">=1.6.0",
        "react-native-tcp-socket": ">=5.3.0"
    },
    "peerDependenciesMeta": {
        "react-native-ssl-pinning": {
            "optional": true
        },
        "react-native-tcp-socket": {
            "optional": true
        }
    },
    "devDependencies": {
        "@inquirer/prompts": "^7.5.1",
        "@types/minimist": "^1.2.5",
        "@types/node": "^24.0.0",
        "@types/pem": "^1.14.4",
        "eslint": "^9.0.0",
        "minimist": "^1.2.8",
        "ts-node": "^10.9.1",
        "tsx": "^4.19.4",
        "typedoc": "^0.28.1",
        "typescript": "^5.7.0",
        "typescript-eslint": "^8.0.0",
        "vitest": "^1.6.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dfpc-coe/node-tak.git"
    },
    "keywords": [
        "tak",
        "atak",
        "wintak",
        "cot",
        "cusor",
        "target",
        "tactical"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dfpc-coe/node-tak/issues"
    },
    "homepage": "https://github.com/dfpc-coe/node-tak#readme"
}
